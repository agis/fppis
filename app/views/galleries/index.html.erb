<div class="ninecol" id="left">
  <% @galleries.each do |gallery| %>
  <h3><%= gallery.title %></h3>
  <ul>
    <% gallery.images.each do |image| %>
    <li><%= image_tag image.file.url(:thumb) %></li>
    <%= link_to 'View Gallery', gallery %>
    <% end %>
  </ul>
  <% end %>
</div>

<div class="threecol last" id="right">
  <%= render 'menu' %>
</div>